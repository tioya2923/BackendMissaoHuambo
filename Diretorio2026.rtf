\pard\plain \s5\f0\fs20\ltrpar\ql\fi0\li0\ri0\sb0\sa0\tx567\tx851\tx1134\facenter\sl288\slmult1\expnd0\expndtw0\cf0\up0\charscalex100\b0\i0\ul0\strike0\accnone\hyphpar0\lang2057
