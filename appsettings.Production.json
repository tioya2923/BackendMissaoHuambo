{
  "ConnectionStrings": {
    "DefaultConnection": "USE_ENVIRONMENT_VARIABLE"
  },
  "Jwt": {
    "Key": "USE_ENVIRONMENT_VARIABLE",
    "Issuer": "USE_ENVIRONMENT_VARIABLE",
    "Audience": "USE_ENVIRONMENT_VARIABLE"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:10000"
      },
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "%ASPNETCORE_HTTPS_CERTIFICATE_PATH%",
          "Password": "%ASPNETCORE_HTTPS_CERTIFICATE_PASSWORD%"
        }
      }
    }
  }
}
